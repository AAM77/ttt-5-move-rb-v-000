#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
puts "Welcome to Tic Tac Toe.";
board = [" ", " ", " ", " ", " ", " ", " ", " ", " "];
display_board(board);

puts "Where would you like to go?";
move_input = gets.strip;
input_to_index(move_input);
move(board, move_input)
